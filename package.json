    "@types/fs-extra": "^9.0.13",
    "@types/lodash": "^4.14.172",
    "@types/lowdb": "^1.0.11",
    "@types/node": "^16.6.2",
    "@types/shelljs": "^0.8.11",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "cpx": "^1.5.0",
    "eslint": "^7.32.0",
    "nodemon": "^2.0.16",
    "ts-node": "^10.2.1",
    "tslint": "^6.1.3",
    "typescript": "^4.4.2"
  }
