{
  "name": "Translate",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && node dist/index.js",
    "dev": "nodemon -e ts,js,ejs,html,css -w . -w public -w views -w routes -w models  src/index.ts --ignore ./import/ --ignore ./export/  --ignore ./src/db/",
    "build": "npm run clean && npm run copy:all && npx mkdirp ./dist/import && tsc",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "excel": "node src/controllers/excel-translates.js",
    "copy:import": "cpx ./import ./dist/",
    "clean": "rimraf ./dist",
    "copy:views": "cpx ./src/views/**/* ./dist/views",
    "copy:public": "cpx ./src/public/**/* ./dist/public",
    "copy:all": "npm run copy:views && npm run copy:public"
  },
  "keywords": [],
  "author": "Muten R≈çshi",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "ejs": "^3.1.3",
    "exceljs": "^3.7.0",
    "express": "^4.17.1",
    "fs": "0.0.1-security",
    "fs-extra": "^10.1.0",
    "http-errors": "^1.8.0",
    "lodash": "^4.17.21",
    "lowdb": "^1.0.0",
    "morgan": "^1.10.0",
    "path": "^0.12.7",
    "rimraf": "^3.0.2",
    "rmfr": "^2.0.0",
    "shelljs": "^0.8.5",
    "sort-json": "^2.0.1",
    "xlsx": "^0.15.1"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
